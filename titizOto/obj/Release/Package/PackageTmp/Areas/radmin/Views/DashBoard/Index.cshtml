@model DashBoardStatistic

@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Areas/radmin/Views/Shared/_LayoutPageAdmin.cshtml";
}

<div class="title">
    <h5>Özet İstatistikler</h5>
</div>
<div class="clleft last">
    <div class="widget">
        <div class="head">
            <h5 class="iMoney">En Son Yapılan Siparişler</h5>
        </div>


        <div class="supTicket nobg">
            <div class="issueSummary">
                <a href="javascript:void(0);" class="floatleft">
                    <img src="~/Areas/radmin/Content/images/user.png" />
                </a>
                <div class="ticketInfo">
                    <ul>
                        <li>Sipariş Durumu:</li>
                        <li class="even"><strong class="green">[ # ]</strong></li>
                        <li>Kullanıcı E-Mail:</li>
                        <li class="even"><a href="#" title="" target="_blank">#</a></li>
                    </ul>
                    <div class="fix"></div>
                </div>
                <div class="fix"></div>
            </div>
        </div>

    </div>
</div>
<div class="clleft">
    <div class="widget">
        <div class="head">
            <h5 class="iChart8">Günlük İstatistikler</h5>
        </div>
        <table cellpadding="0" cellspacing="0" width="100%" class="tableStatic">
            <thead>
                <tr>
                    <td width="21%">Toplam</td>
                    <td>Açıklama</td>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td align="center"><a href="#" title="" class="webStatsLink"><span></span></a></td>
                    <td><b>Günlük Sipariş Adeti</b></td>
                </tr> 
                <tr>
                    <td align="center"><a href="#" title="" class="webStatsLink"><span></span></a></td>
                    <td>Kullanıcı</td>
                </tr>
                <tr>
                    <td align="center"><a href="#" title="" class="webStatsLink"><span></span></a></td>
                    <td>Bülten Abonesi</td>
                </tr>
             
            </tbody>
        </table>
    </div>
</div>
<div class="clleft">
    <div class="widget">
        <div class="head">
            <h5 class="iChart8">Özet İstatistikler</h5>
        </div>
        <table cellpadding="0" cellspacing="0" width="100%" class="tableStatic">
            <thead>
                <tr>
                    <td width="21%">Toplam</td>
                    <td>Açıklama</td>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td align="center"><a href="#" title="" class="webStatsLink"><span></span></a></td>
                    <td><b>Toplam Sipariş Adeti</b></td>
                </tr>
                <tr>
                    <td align="center"><a href="#" title="" class="webStatsLink"><span></span></a></td>
                    <td>Ürün Kategori</td>
                </tr>
                <tr>
                    <td align="center"><a href="#" title="" class="webStatsLink"><span></span></a></td>
                    <td>Ürün</td>
                </tr>
                <tr>
                    <td align="center"><a href="#" title="" class="webStatsLink"><span></span></a></td>
                    <td>Dinamik Sayfa</td>
                </tr>
                <tr>
                    <td align="center"><a href="#" title="" class="webStatsLink"><span></span></a></td>
                    <td>Kullanıcı</td>
                </tr>
                <tr>
                    <td align="center"><a href="#" title="" class="webStatsLink"><span></span></a></td>
                    <td>Bülten Abonesi</td>
                </tr>
               
            </tbody>
        </table>
    </div>
</div>


<div class="fix"></div>

