    @RenderSection("cssArea", false)

</head>
<body>

    @Html.Partial("adminHeader", (AdminVariable)ViewBag.adminVariable)

    <!-- Content wrapper -->
    <div class="wrapper">

        <!-- Left navigation -->
        @Html.Hidden("activeController", ViewContext.RouteData.Values["Controller"])

        <div class="leftNav">
            <ul id="menu">

            <li class="dash"><a href="@Url.Action("Index", "Dashboard")" title=""  data-controller="">
                    <span>Anasayfa</span></a></li>
                <li class="files"><a href="@Url.Action("Index", "Category")" title=""  data-controller="">
                    <span>Kategori</span></a></li>

                <li class="slider"><a href="@Url.Action("Index", "Slider")" title=""  data-controller="">
                    <span>Slider</span></a></li>



                <li class="car"><a href="#" title="" class="exp" data-controller=""><span>Araç Marka
                    / Araç Model </span></a>
                    <ul class="sub">
                        <li><a href="@Url.Action("Index", "CarBrand")" title="Araç Marka">Araç Marka</a>
                        </li>
                        <li><a href="@Url.Action("Index", "CarModel")" title="Araç Model">Araç Model</a>
                        </li>
                    </ul>
                </li>

                <li class="product"><a href="#" title="" class="exp" data-controller=""><span>Ürün Yönetimi
                </span></a>
                    <ul class="sub">
                        <li><a href="@Url.Action("Index", "Product")" title="Ürünler">Ürünler</a>
                        </li>
                        <li><a href="@Url.Action("Index", "Critear")" title="Ürün Seçenekleri">Ürün Seçenekleri</a>
                        </li>
                        <li><a href="@Url.Action("Index", "Brand")" title="Ürün Markası">Ürün Markası</a>
                        </li>
                        <li><a href="@Url.Action("Index", "Business")" title="Bayiler">Bayiler</a>
                        </li>

                    </ul>
                </li>
                 
                <li class="create"><a href="@Url.Action("Index", "Page")" title=""  data-controller="">
                    <span>İçerik Sayfaları</span></a></li> 
            </ul>
        </div>

        <!-- Content -->
        <div class="content">
            @RenderBody()
        </div>

        <div class="fix"></div>
    </div>

    <!-- Footer -->
    <div id="footer">
        <div class="wrapper">
            <span>&copy; Copyright 2014. All rights reserved. </span>
        </div>
    </div>

    <script type="text/javascript">
        var mainPath = "@Url.Content("~/")";
    </script>
