@model ViewModel.AllBrand.helperAllBrand

@{
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
}

<div class="wrapper">
    <div class="generalMainContainer">

        <div class="generalMainHeader contentBg">
        </div>
        <div class="generalMainContent contentBg">

            <div class="innerContent">


                @Html.Partial("breadCrumb", Model.breadCrumbItem)

                <h1 class="mainHeader">@Model.brandName</h1>

                <div class="clearfix"></div>

                <ul class="allBrandList">

                    @foreach (var item in Model.brandList)
                    {
                        <li>
                            <a href="@item.url" title="@item.name">
                                <img src="@item.photo" />
                                <span>@item.name</span>
                            </a>
                        </li>
                    } 

                </ul>
                <div class="clearfix"></div>
            </div>
        </div>
        <div class="generalMainFooter contentBg"></div>

    </div>
</div>

@section scriptArea{


    <script id="titizJs" src="~/Content/js/core.js?modul=brandList"></script>

}