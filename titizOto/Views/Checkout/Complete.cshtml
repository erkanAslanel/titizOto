@model ViewModel.Checkout.Complete.helperComplete

@{
  
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
}


<div class="wrapper">
    <div class="generalMainContainer">
        <div class="generalMainHeader contentBg">
        </div>
        <div class="generalMainContent contentBg">
            <div class="innerContent">
                <div class="clearfix"></div>

                <div class="checkoutContainer orderSuccessfulContainer">

                    <h1 class="mainHeader staticHeader mLeft10">@Model.title</h1>

                    <div class="pageContent checkoutStaticContent mLeft10">

                        <p class="checkoutDetail">@Html.Raw(Model.detail)</p>


                        @if (Model.isTransferOrder)
                        {
                            <div class="transferInfoContainer">
                                <b>@Resources.lang.orderTransferInfoTitle</b>

                                <p>@Resources.lang.orderTransferInfo</p>

                                @Html.Raw(Model.transferAcountHtml)

                            </div>
                        }

                    </div>

                    <div class="clearfix"></div>

                </div>

                <div class="clearfix"></div>
            </div>
        </div>
        <div class="generalMainFooter contentBg">
        </div>
    </div>
</div>

@section scriptArea{

    <script id="titizJs" src="~/Content/js/core.js?modul=complete"></script>

}
